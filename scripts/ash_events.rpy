    f "Eu- eu não posso acreditar... isso foi real!"

# game/scripts/ash_events.rpy:2721
translate PortuguesBR ash_twinbackcol_cb3e26e0_1:

    # a "Hehe, yup!"
    a "Hehe, sim!"

# game/scripts/ash_events.rpy:2722
