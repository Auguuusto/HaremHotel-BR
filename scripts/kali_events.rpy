    k "{size=-10}Eu sabia que não seria uma boa ideia...{/size}"

# game/scripts/kali_events.rpy:5292
translate PortuguesBR k_fancyq_1d05beb7:

    # c "Come with me."
    c "Venha comigo."

# game/scripts/kali_events.rpy:5294
translate PortuguesBR k_fancyq_0f4f8ef4:

    # k "{size=-10}Where are we going...?{/size}"
    k "{size=-10}Para onde vamos...?{/size}"

# game/scripts/kali_events.rpy:5297
translate PortuguesBR k_fancyq_c57f3d6e:

