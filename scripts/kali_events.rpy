    k "O... banheiro...?"

# game/scripts/kali_events.rpy:5301
translate PortuguesBR k_fancyq_31dcab63:

    # k "Hahhh..."
    k "Hahhh..."

# game/scripts/kali_events.rpy:5303
translate PortuguesBR k_fancyq_a110451b:

