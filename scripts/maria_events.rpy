# game/scripts/maria_events.rpy:2115
translate PortuguesBR mar_rebound_47c51c49_1:

    # m "And the best part is, {i}you're{/i} the one with dark elf eyes, but {u}I'm{/u} the one who gets punished...!"
    m "E a melhor parte é que {i}você é{/i} aquele com olhos escuros de elfo, mas {u}sou{/u} aquele que é punido...!"

# game/scripts/maria_events.rpy:2117
translate PortuguesBR mar_rebound_abd2ef91_1:

