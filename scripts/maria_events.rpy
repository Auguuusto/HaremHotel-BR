    a "Isso não é verdade, Maria...! A gente fica triste do mesmo jeito..."

# game/scripts/maria_events.rpy:2120
translate PortuguesBR mar_rebound_e612037c_4:

    # m "..."
    m "..."

# game/scripts/maria_events.rpy:2121
