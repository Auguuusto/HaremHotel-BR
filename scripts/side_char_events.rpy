# game/scripts/side_char_events.rpy:4006
translate PortuguesBR love_nia_5_2a1101cd_1:

    # c "Iksa is alive too, I met her not too long ago."
    c "Iksa também está viva, eu a conheci há pouco tempo."

# game/scripts/side_char_events.rpy:4007
translate PortuguesBR love_nia_5_39561605_3:

    # ghen "If Iksa is still alive, then-!"
    ghen "Se Iksa ainda está vivo, então-!"

# game/scripts/side_char_events.rpy:4008
