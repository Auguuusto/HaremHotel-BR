translate PortuguesBR love_nia_4_a6da29fc:

    # l "Okay! Be right back!"
    l "Ok! Volto logo!"

# game/scripts/side_char_events.rpy:3480
translate PortuguesBR love_nia_4_a8ff37b4:

    # nia "Ergh... am I honoring our ancestors correctly...?"
    nia "Ergh... estou honrando nossos ancestrais corretamente...?"

# game/scripts/side_char_events.rpy:3481
translate PortuguesBR love_nia_4_d3173cf0:

    # l "Well enough~"
    l "Bem o suficiente ~"

