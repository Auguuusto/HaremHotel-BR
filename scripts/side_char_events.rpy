# game/scripts/side_char_events.rpy:3474
translate PortuguesBR love_nia_4_6c677e5b:

    # nia "Ummm, no thanks... I think I'll pass. That sounds really tedious..."
    nia "Ummm, não, obrigado... Acho que vou passar. Isso soa realmente tedioso..."

