    nia "*suspirar*..."

# game/scripts/side_char_events.rpy:2620
translate PortuguesBR love_nia_1_05de1b4a_1:

    # nia "You're sweet, Lin."
    nia "Você é um doce, Lin."

# game/scripts/side_char_events.rpy:2621
translate PortuguesBR love_nia_1_b2eb6f38_1:

