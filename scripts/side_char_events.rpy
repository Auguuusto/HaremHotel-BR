translate PortuguesBR vanessa1_8fe83781:

    # c "I was there when your photos were put on the projector."
    c "Eu estava lá quando suas fotos foram colocadas no projetor."

# game/scripts/side_char_events.rpy:9117
translate PortuguesBR vanessa1_bd315f98:

    # van "Oh... right..."
    van "Oh, certo..."

# game/scripts/side_char_events.rpy:9118
