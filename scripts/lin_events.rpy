    # l "Good morning, [linoname]~"
    l "Bom dia, [linoname!t]~"

# game/scripts/lin_events.rpy:2675
translate PortuguesBR lin_boat2_cc4ac566:

    # c "Oh, hey Lin."
    c "Oi Lin."

# game/scripts/lin_events.rpy:2676
translate PortuguesBR lin_boat2_44cf66c7:

    # l "Tea?"
    l "Chá?"

# game/scripts/lin_events.rpy:2677
